{
    "name": "marussia/content-field",
    "description": "Marussia content fields",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "content-field"
    ],
    "authors": [
        {
            "name": "Mikhail Ilinsky",
            "email": "mail@milinsky.ru"
        }
    ],
    "support": {
        "docs": "https://docs.marussia.org/content-field/"
    },
    "require": {
        "php": "^7.2",
        "marussia/dependency-injection": "~1.0"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Marussia\\ContentField\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Marussia\\ContentField\\Test\\": ["tests/"]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4",
        "mockery/mockery": "^1.2"
    }
}
